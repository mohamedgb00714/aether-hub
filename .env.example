# aethermsaid hub Environment Variables Template
# Copy this file to .env and fill in your keys

# --- AI Providers ---
# Google Gemini (https://aistudio.google.com/)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter (https://openrouter.ai/)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenAI (https://platform.openai.com/)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic (https://console.anthropic.com/)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# --- Other Services ---
# Resend (https://resend.com/api-keys)
RESEND_API_KEY=your_resend_api_key_here

# --- Security ---
# Optional: Set a specific encryption key for development
# If not set, a secure key will be generated locally via safeStorage
# AETHER_ENCRYPTION_KEY=your_custom_encryption_key

# --- Feature Flags (Disabled by default for safety) ---
# Enable unofficial automation integrations (use at your own risk)
ENABLE_WHATSAPP=false
ENABLE_DISCORD=false
ENABLE_TELEGRAM=false
